<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="studentselector">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.uibinder.UiBinder" />

<!-- Hier muss das Datenbanktool eingebunden sein. -->
	<inherits name='com.google.code.morphia.Morphia' />

	<source path="client" />
	<source path="shared" />

<!-- Der Entrypoint. Davon gibt es nur einen. -->
	<entry-point class="de.macbarfuss.studentselector.client.SelectorUi" />
</module>